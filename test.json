{
    "executionRoleArn": "arn:aws:iam::340638733511:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "nodejs-sample-app",
            "image": "340638733511.dkr.ecr.us-east-2.amazonaws.com/nodejs-sam-app:latest",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "protocol": "tcp",
                    "containerPort": 80
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "EC2"
    ],
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512",
    "family": "nodejs"
}